<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>${app.id}</application>
    <version>${buildNumber}</version>
    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>

        <property name="origin" value="${origin}"/>
        <property name="host" value="${host}"/>
        <property name="login.url" value="${login.url}"/>
        <property name="login.referer" value="${login.referer}"/>
        <property name="login.data" value="${login.data}"/>
        <property name="download.url" value="${download.url}"/>
        <property name="download.referer" value="${download.referer}"/>
        <property name="download.data" value="${download.data}"/>
        <property name="accounts.url" value="${accounts.url}"/>
        <property name="accounts.referer" value="${accounts.referer}"/>
        <property name="admin" value="${admin}"/>
        <property name="recipients" value="${recipients}"/>
        <property name="app.version" value="${app.version}"/>
        <property name="waitTimeMillis" value="${waitTimeMillis}"/>
    </system-properties>
</appengine-web-app>
